
str title = hello_world()

func header = (bool input, str output) str {
  if input == true {
    return  "bye world"
  }

  if output == nil {
    return "hello world"
  }

  return output
}

str term = $`echo $TERM`

html {
  head > title > @{title}
  body {
    h1 > @{header(false)}
    div > @{term}
  }
}
