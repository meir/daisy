
meta {
  str url = "/test"
}

str title = "Website 1.0"

str line_content = "This is a list item"
str css = use("assets/main.scss")

element line = : li > @{line_content}

element lines = : ul {
  @{line}
  @{line}
  @{line}
}

return : html {
  head {
    title > @{title}
    link rel="stylesheet" href=(css);
  }

  body {
    h1 > ''
      Hello
        world!
    ''
    @{lines}
  }
}
